{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "maximos_invitation/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/maximos_invitation/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "maximos_invitation/wsgi.py"
    }
  ]
}